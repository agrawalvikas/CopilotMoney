# Copy this file to .env.local and set the values.

# --- Clerk Authentication ---
# Found in Clerk Dashboard → API Keys.
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY="pk_test_..."
CLERK_SECRET_KEY="sk_test_..."

# --- API ---
# The base URL of the backend API server.
# In development this is typically http://localhost:3001
NEXT_PUBLIC_API_BASE_URL="http://localhost:3001"

# --- Teller.io (Bank connections — US) ---
# Found in the Teller Dashboard.
NEXT_PUBLIC_TELLER_APP_ID="app_..."
# Environment: sandbox | development | production
NEXT_PUBLIC_TELLER_ENV="sandbox"
